@model List<Kid>

<div class="col-md-6">
    <div class="dash_blog">
        <div class="dash_blog_inner">
            <div class="dash_head">
                <h3><span><i class="fa fa-calendar"></i>&nbsp; Son Kayıt Olan Çocuklar</span><span class="plus_green_bt"><a href="/Kid/Index">+</a></span></h3>
            </div>
            <div class="task_list_main">
                <ul class="task_list">
                    @foreach (var item in Model.OrderByDescending(x => x.RegisterDate).Take(6))
                    {
                        <li><a href="#" class="text-dark">Ebeveyn : @item.Parents.Name - Öğrenci : @item.Name</a><br><strong>@item.RegisterDate.ToString("dd MMMM yy")</strong></li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>